---
layout: null
pagination:
  data: collections.redirects
  size: 1
  alias: redirect
permalink: "{{ redirect.from }}index.html"
---
<!doctype html>
<html lang="ru">
  <head>
    <meta charset="utf-8">
    <title>Redirecting…</title>
    <meta http-equiv="refresh" content="0;url={{ redirect.to }}">
    <link rel="canonical" href="{{ redirect.to }}">
  </head>
  <body>
    <script>window.location.replace('{{ redirect.to }}');</script>
    <a href="{{ redirect.to }}">Перейти к статье</a>
  </body>
</html>
